{
  "Name": "Superior Steam",
  "Version": "0.1.0.0",
  "Author": "humhunch",
  "Actions": [
    {
      "Name": "Big picture",
      "UUID": "com.humhunch.superior-steam.big-picture",
      "Icon": "imgs/actions/steam/big_screen_off",
      "Tooltip": "Manage Steam's Big Picture mode.",
      "PropertyInspectorPath": "ui/increment-counter.html",
      "Controllers": ["Keypad"],
      "States": [
        {
          "Image": "imgs/actions/steam/big_screen_off",
          "TitleAlignment": "middle"
        },
        {
          "Image": "imgs/actions/steam/big_screen_on",
          "TitleAlignment": "middle"
        }
      ]
    },
    {
      "Name": "App",
      "UUID": "com.humhunch.superior-steam.run-app",
      "Icon": "imgs/actions/steam/y",
      "Tooltip": "Installed Steam games.",
      "PropertyInspectorPath": "ui/app.html",
      "Controllers": ["Keypad"],
      "States": [
        {
          "Image": "imgs/actions/steam/y",
          "TitleAlignment": "middle"
        }
      ]
    },
    {
      "Name": "Launch account",
      "UUID": "com.humhunch.superior-steam.launch-account",
      "Icon": "imgs/actions/steam/default_user",
      "Tooltip": "Launch Steam with a specific account.",
      "PropertyInspectorPath": "ui/launch-account.html",
      "Controllers": ["Keypad"],
      "States": [
        {
          "Image": "imgs/actions/steam/default_user",
          "TitleAlignment": "middle"
        }
      ]
    }
  ],
  "Category": "Superior Steam",
  "CategoryIcon": "imgs/plugin/category-icon",
  "CodePath": "bin/plugin.js",
  "Description": "Stream deck plugin to manage Steam",
  "Icon": "imgs/plugin/marketplace",
  "SDKVersion": 2,
  "Software": {
    "MinimumVersion": "6.5"
  },
  "OS": [
    {
      "Platform": "windows",
      "MinimumVersion": "10"
    }
  ],
  "Nodejs": {
    "Version": "20",
    "Debug": "--inspect=9229"
  },
  "UUID": "com.humhunch.superior-steam",
  "ApplicationsToMonitor": {
    "windows": ["steam.exe"]
  }
}
